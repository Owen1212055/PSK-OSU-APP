{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "credentialsSource": "local"
      },
      "ios": {
        "credentialsSource": "local"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "credentialsSource": "local"
      },
      "ios": {
        "credentialsSource": "local"
      }
    },
    "production": {
      "android": {
        "credentialsSource": "local"
      },
      "ios": {
        "credentialsSource": "local"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6743056561",
        "ascApiKeyId": "$ASC_API_KEY_ID",
        "ascApiKeyIssuerId": "$ASC_API_KEY_ISSUER_ID",
        "ascApiKeyPath": "ios/certs/AuthKey.p8"
      }
    }
  }
}
